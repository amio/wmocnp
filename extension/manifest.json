{
	"name": "WMO-SpellCN",
	"version": "0.1",
	"manifest_version": 2,
	"description": "将 wmo.178.com 中鼠标指向技能的英文说明替换为中文说明",
	"permissions": [
		"http://wmo.178.com/"
	],
	"icons": { "16": "icon16.png",
	           "48": "icon48.png",
	          "128": "icon128.png" },
	"content_scripts": [
		{
			"matches": ["http://wmo.178.com/*"],
			"css": ["wmocn.css"],
			"js": ["main.js"]
		}
	],
	"web_accessible_resources": [
		"wmocn.js"
	]
}